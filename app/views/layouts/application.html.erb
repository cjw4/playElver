<!DOCTYPE html>
<html>
<head>
  <title>CodeName1</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id="wrapper">
  <div id="navigation">
	<ul>
		<li><%= link_to "Home", root_path %></li>
		<li><%= link_to "Scorecard", scorecards_path %></li>
	</ul>
  </div><!-- end navigation div-->

  <% flash.each do |name, msg| %>
  	<%= content_tag :div, msg, :id => "flash_#{name}" %>
  <% end %>
  
  <%= yield %>
</div><!-- end wrapper div -->
</body>
</html>
